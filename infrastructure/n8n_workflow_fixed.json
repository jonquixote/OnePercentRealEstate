            "name": "Split Batches",
            "type": "n8n-nodes-base.itemLists",
            "typeVersion": 1,
            "position": [
                1150,
                -100
            ],
            "id": "8dd7edb2-0b84-48a3-934f-64d34bf00708"
        },
        {
            "parameters": {
                "operation": "executeQuery",
                "query": "={{ \"INSERT INTO listings (address, price, bedrooms, bathrooms, sqft, latitude, longitude, listing_type, listing_status, raw_data, zip_code, city, state, url, property_url, listing_date) VALUES ('\" + ($json.address || $json.formatted_address || 'Unknown').replace(/'/g, \"''\") + \"', \" + ($json.list_price !== undefined && $json.list_price !== null ? $json.list_price : 'NULL') + \", \" + ($json.beds !== undefined && $json.beds !== null ? $json.beds : 'NULL') + \", \" + ($json.full_baths !== undefined && $json.full_baths !== null ? $json.full_baths : 'NULL') + \", \" + ($json.sqft !== undefined && $json.sqft !== null ? $json.sqft : 'NULL') + \", \" + ($json.latitude !== undefined && $json.latitude !== null ? $json.latitude : 'NULL') + \", \" + ($json.longitude !== undefined && $json.longitude !== null ? $json.longitude : 'NULL') + \", 'for_sale', 'FOR_SALE', '\" + JSON.stringify($json).replace(/'/g, \"''\") + \"'::jsonb, '\" + ($json.zip_code || '') + \"', '\" + ($json.city || '').replace(/'/g, \"''\") + \"', '\" + ($json.state || '') + \"', '\" + ($json.property_url || '') + \"', '\" + ($json.property_url || '') + \"', NOW()) ON CONFLICT (address, listing_type, listing_date) DO UPDATE SET price = EXCLUDED.price, bedrooms = EXCLUDED.bedrooms, bathrooms = EXCLUDED.bathrooms, sqft = EXCLUDED.sqft, latitude = EXCLUDED.latitude, longitude = EXCLUDED.longitude, raw_data = EXCLUDED.raw_data, updated_at = NOW();\" }}",
                "additionalFields": {}
            },
            "name": "Save Listing",
            "type": "n8n-nodes-base.postgres",
            "typeVersion": 1,
            "position": [
                1350,
                -100
            ],
            "id": "2081e831-f950-44b3-aa38-5e65c5afdf66",
            "credentials": {
                "postgres": {
                    "id": "pR4wUtye9OhGE0WS",
                    "name": "Postgres account"
                }
            }
        },
        {
            "parameters": {
                "operation": "executeQuery",
                "query": "={{ \"UPDATE crawl_jobs SET status = 'completed', completed_at = NOW(), items_found = \" + ($items(\"Scraper Service\")[0].json.results ? $items(\"Scraper Service\")[0].json.results.length : 0) + \" WHERE id = '\" + $items(\"Get Next Job\")[0].json.id + \"';\" }}",
                "additionalFields": {}
            },
            "name": "Mark Complete",
            "type": "n8n-nodes-base.postgres",
            "typeVersion": 1,
            "position": [
                1600,
