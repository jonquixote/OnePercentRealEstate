                    "id": "pR4wUtye9OhGE0WS",
                    "name": "Postgres account"
                }
            }
        },
        {
            "parameters": {
                "operation": "executeQuery",
                "query": "={{ \"UPDATE crawl_jobs SET status = 'failed', error_message = '\" + ($node[\"Scraper Service\"].error ? $node[\"Scraper Service\"].error.message : \"Unknown Error\") + \"' WHERE id = '\" + $items(\"Get Next Job\")[0].json.id + \"';\" }}",
                "additionalFields": {}
            },
            "name": "Mark Failed",
            "type": "n8n-nodes-base.postgres",
            "typeVersion": 1,
            "position": [
                900,
