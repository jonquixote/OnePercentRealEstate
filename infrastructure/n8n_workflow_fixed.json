{
    "name": "US Real Estate Scraper Orchestrator",
    "nodes": [
        {
            "parameters": {
                "rule": {
                    "interval": [
                        {
                            "field": "seconds",
                            "secondsInterval": 30
                        }
                    ]
                }
            },
            "name": "Schedule",
            "type": "n8n-nodes-base.scheduleTrigger",
            "typeVersion": 1,
            "position": [
                0,
                0
            ],
            "id": "847c3b6a-51d0-4909-978a-087c5ed6ae6b"
        },
        {
            "parameters": {
                "operation": "executeQuery",
                "query": "UPDATE crawl_jobs \nSET status = 'processing', started_at = NOW() \nWHERE id = (\n  SELECT id \n  FROM crawl_jobs \n  WHERE status = 'pending' \n  ORDER BY region_value ASC \n  LIMIT 1 \n  FOR UPDATE SKIP LOCKED\n)\nRETURNING *;",
                "additionalFields": {}
            },
            "name": "Get Next Job",
            "type": "n8n-nodes-base.postgres",
            "typeVersion": 1,
            "position": [
                224,
                0
            ],
            "id": "56871ec6-b095-4346-815d-2afef8254c93",
            "credentials": {
                "postgres": {
                    "id": "pR4wUtye9OhGE0WS",
                    "name": "Postgres account"
                }
            }
        },
        {
            "parameters": {
                "conditions": {
                    "boolean": [
                        {
                            "value1": "={{ $json.id }}",
                            "operation": "notEqual",
                            "value2": "undefined"
                        }
                    ]
                }
            },
            "name": "Job Found?",
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                448,
                0
