            "id": "6cf6bcdb-4df2-4354-8d34-de19718cd626"
        },
        {
            "parameters": {
                "requestMethod": "POST",
                "url": "http://scraper:8000/scrape",
                "jsonParameters": true,
                "options": {},
                "bodyParametersJson": "={{ JSON.stringify({\n  \"location\": $json.region_value,\n  \"listing_type\": \"for_sale\",\n  \"past_days\": 30\n}) }}"
            },
            "name": "Scraper Service",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 1,
            "position": [
                672,
                0
            ],
            "id": "4b8d9173-8acc-46f5-a2cd-efd698e16855",
            "continueOnFail": true
        },
        {
            "parameters": {
                "conditions": {
                    "boolean": [
                        {
                            "value1": "={{ $json.results && $json.results.length > 0 }}",
                            "operation": "equal",
                            "value2": true
                        }
                    ]
                }
            },
            "name": "Has Results?",
            "type": "n8n-nodes-base.if",
            "typeVersion": 1,
            "position": [
                900,
                0
